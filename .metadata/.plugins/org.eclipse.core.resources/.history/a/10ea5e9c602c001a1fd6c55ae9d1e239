package com.shubham;

public class InventoryValues {
private String name;
private String weight;
private int price;
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public String getWeight() {
	return weight;
}
public void setWeight(String weight) {
	this.weight = weight;
}
public int getPrice() {
	return price;
}
public void setPrice(int price) {
	this.price = price;
}
}
